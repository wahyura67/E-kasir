<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Coffeshop">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="250dp"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="1">

        <LinearLayout
            android:id="@+id/home_mRide"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_margin="10dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="0.33"
            android:clickable="true"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp"
            tools:ignore="MissingPrefix">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFCC00">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView3"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:scaleType="center" />

                <TextView
                    android:id="@+id/textView3"
                    fontPath="fonts/Poppins-SemiBold.otf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:maxLines="1"
                    android:text="Sayur Asem"
                    android:textColor="#252525"
                    android:textSize=""
                    android:textStyle="bold"
                    tools:ignore="MissingPrefix" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_mSend"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_margin="10dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="0.33"
            android:clickable="true"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp"
            tools:ignore="MissingPrefix">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFCC00">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView2"
                    android:layout_centerHorizontal="true"

                    android:layout_marginBottom="5dp"
                    android:scaleType="center"
                    android:src="@drawable/ic_esend" />

                <TextView

                    android:id="@+id/textView2"
                    fontPath="fonts/Poppins-SemiBold.otf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:maxLines="1"
                    android:text="Sayur Lodeh"
                    android:textColor="#252525"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold"
                    tools:ignore="MissingPrefix" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_mCar"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_margin="10dp"
            android:layout_marginLeft="5dp"
            android:layout_weight="0.33"
            android:clickable="true"
            app:cardCornerRadius="10dp"
            app:cardElevation="5dp"
            tools:ignore="MissingPrefix">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#FFCC00">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView6"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"

                    android:scaleType="center"
                    android:src="@drawable/ic_ecar" />

                <TextView

                    android:id="@+id/textView6"
                    fontPath="fonts/Poppins-SemiBold.otf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"

                    android:maxLines="1"
                    android:text="Nasi Goreng"

                    android:textColor="#252525"
                    android:textSize="@dimen/menu_fontSize"
                    android:textStyle="bold"
                    tools:ignore="MissingPrefix" />

            </RelativeLayout>


        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:weightSum="1">

        <LinearLayout
