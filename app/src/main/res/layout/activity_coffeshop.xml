                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView055"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="5dp"
                    android:scaleType="center"
                    android:src="@drawable/ic_ebox"
