<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/warnabg"
    android:orientation="vertical"
    tools:context=".Landingpage">

    <ImageView
        android:layout_width="369dp"
        android:layout_height="472dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="150dp"
        android:background="@drawable/grup3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="156dp" />

    <TextView
        android:layout_width="258dp"
        android:layout_height="53dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="350dp"
        android:text="@string/ekasir"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="354dp" />


    <TextView
        android:layout_width="120dp"
        android:layout_height="33dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="400dp"
        android:layout_marginBottom="25dp"
        android:text="Category:" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="188dp"
        android:layout_height="43dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center"
        android:layout_marginTop="400dp"
        android:entries="@array/usaha"
        android:padding="20dp"
        android:text="Usaha"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="111dp"
        tools:layout_editor_absoluteY="422dp"></Spinner>

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="440dp"
        android:background="#FFCC00"
        android:onClick="cuci"
        android:text="Cuci Kendaraan"
        tools:ignore="MissingConstraints,OnClick,OnClick"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="473dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="114dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="490dp"
        android:background="#FFCC00"
        android:onClick="cuci"
        android:text="Coffee Shop"
        tools:ignore="MissingConstraints,OnClick,OnClick"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="473dp" />

</RelativeLayout>